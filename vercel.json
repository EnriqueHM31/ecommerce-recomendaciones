{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "75mb"
            }
        },
        {
            "src": "index.html",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "command": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/index.html"
        }
    ]
}